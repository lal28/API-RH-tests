{
  "name": "api-rh-tests",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "code:check": "npm run lint && npm run format:check",
    "code:fix": "npm run lint:fix && npm run format",
    "test": "set NODE_ENV=test&& node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "set NODE_ENV=test&& node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "set NODE_ENV=test&& node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "REST",
    "API",
    "Express",
    "Top-RH"
  ],
  "author": "Lucas, Pedro, Jaine",
  "license": "ISC",
  "description": "API RESTful do sistema rh",
  "dependencies": {
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "swagger-ui": "^5.28.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@eslint/js": "^9.36.0",
    "@faker-js/faker": "^10.0.0",
    "@jest/globals": "^30.1.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
